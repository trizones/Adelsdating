@using Web.Models
@model ApplicationUser
@{
    ViewBag.Title = "MyPage";
}
<div class="MyPage">
    <div class="UserNamePic">
        <h1>@Html.Raw((ViewData["nickName"]))</h1>
        <img src="@Url.Action("ProfilePicture", "Home" )" style="width:300px;height:300px; background: #FFFFFF;
    margin: auto;
    padding: 6px;
    box-shadow: 0px 0px 20px #888;" />
    </div>
    <div class="UserInformation">
        <ol>
            <li>Namn: @Html.Raw((ViewData["fullName"])) </li>
        </ol>
    </div>
   

</div>



