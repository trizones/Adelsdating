@model Web.Controllers.PostFriendRequestViewModel

@{
    ViewBag.Title = "Vänförfrågningar";
}

<h2>Dina Förfrågningar</h2>


<table class="table">
    <tr>
        <th>
            @*Html.DisplayNameFor(model => model.Requests) *@
            
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Requests) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FromUser.Nickname) :    @Html.ActionLink("Acceptera förfrågan?", "AcceptRequest", "FriendRequest", routeValues: new { requestId = item.Id}, htmlAttributes: null)
        </td>
    </tr>
}

</table>
